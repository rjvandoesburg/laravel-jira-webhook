{
    "name": "rjvandoesburg/laravel-jira-webhook",
    "description": "An endpoint for your Laravel application to listen to Atlassian's Jira webhook events ",
    "keywords": ["laravel", "laravel-5-package", "atlassian", "atlassian-jira", "jira"],
    "license": "MIT",
    "authors": [
        {
            "name": "Robert-John van Doesburg",
            "email": "rjvandoesburg@gmail.com"
        }
    ],
    "require": {
        "php" : ">=5.6.0",
        "illuminate/support": "^5.3",
        "illuminate/events": "^5.3",
        "illuminate/http": "^5.3",
        "illuminate/container": "^5.3",
        "illuminate/config": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "mockery/mockery": "^0.9.9",
        "laracasts/testdummy": "^2.3",
        "laravel/laravel": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Atlassian\\JiraWebhook\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
